    "ts-node-maintained": "^10.9.6",
    "typescript": "~5.8"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.5.1",
    "@adonisjs/cache": "^1.3.1",
    "@adonisjs/core": "^6.19.3",
